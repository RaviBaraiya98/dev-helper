{
  "name": "@ravibaraiya/dev-helper",
  "version": "2.2.1",
  "description": "A beginner-friendly CLI tool that helps developers set up projects and understand errors (READ-ONLY, never executes project code)",
  "main": "bin/index.js",
  "bin": {
    "dev-helper": "./bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "node bin/index.js --help",
    "test:safety": "node -e \"require('./utils/safety').isCommandSafe('npm start')\" && echo Safety module loaded",
    "link": "npm link",
    "unlink": "npm unlink -g dev-helper"
  },
  "keywords": [
    "cli",
    "developer-tools",
    "git",
    "setup",
    "beginner-friendly",
    "nodejs",
    "python",
    "java",
    "rust",
    "go",
    "docker",
    "safe",
    "read-only"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "bin/",
    "commands/",
    "detectors/",
    "checkers/",
    "fixes/",
    "git/",
    "utils/",
    "data/"
  ]
}
